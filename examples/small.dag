int f1(int x) {
  return x;
}

int[] f2(int[] x) {
  return x;
}

struct circle {
  int radius;
  int x;
  int y;
};

circle f3() {
  return struct circle {
    x = 0;
    y = 0;
    radius = 3;
  };
}

int f4() {
  circle x = struct circle {
    x = 0;
    y = 0;
    radius = 3;
  };
  return x.radius;
}

circle[] f5(int[] xs) {
  return parallel(int x <- xs) {
    return struct circle {
      x = 0;
      y = 0;
      radius = x;
    };
  };
}

int[] f6(int[] xs) {
  return parallel(int x <- xs) {
    return x;
  };
}
